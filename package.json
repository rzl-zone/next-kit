{
  "name": "@rzl-zone/next-kit",
  "version": "0.7.0",
  "type": "module",
  "description": "A flexible and extensible Next.js toolkit for modern fullstack app development, featuring utilities, helpers, and conventions tailored for clean code architecture.",
  "license": "MIT",
  "author": "Rizalvin Dwiky <rizalvindwiky1998@gmail.com>",
  "homepage": "https://github.com/rzl-zone/next-kit",
  "engineStrict": true,
  "types": "./dist/index.d.ts",
  "typescript": {
    "projectRoot": "."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    },
    "./extra": {
      "types": "./dist/extra/index.d.ts",
      "import": "./dist/extra/index.js",
      "require": "./dist/extra/index.cjs"
    },
    "./extra/action": {
      "types": "./dist/extra/action.d.ts",
      "import": "./dist/extra/action.js",
      "require": "./dist/extra/action.cjs"
    },
    "./extra/context": {
      "types": "./dist/extra/context.d.ts",
      "import": "./dist/extra/context.js",
      "require": "./dist/extra/context.cjs"
    },
    "./extra/pathname": {
      "types": "./dist/extra/pathname.d.ts",
      "import": "./dist/extra/pathname.js",
      "require": "./dist/extra/pathname.cjs"
    },
    "./hoc": {
      "types": "./dist/hoc/index.d.ts",
      "import": "./dist/hoc/index.js",
      "require": "./dist/hoc/index.cjs"
    },
    "./themes": {
      "types": "./dist/themes/index.d.ts",
      "import": "./dist/themes/index.js",
      "require": "./dist/themes/index.cjs"
    },
    "./themes/pages-dir": {
      "types": "./dist/themes/pages-dir.d.ts",
      "import": "./dist/themes/pages-dir.js",
      "require": "./dist/themes/pages-dir.cjs"
    },
    "./progress-bar": {
      "types": "./dist/progress-bar/index.d.ts",
      "import": "./dist/progress-bar/index.js",
      "require": "./dist/progress-bar/index.cjs"
    },
    "./progress-bar/default.css": "./dist/progress-bar/default.css",
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "extra": [
        "dist/extra/index.d.ts"
      ],
      "extra/action": [
        "dist/extra/action.d.ts"
      ],
      "extra/context": [
        "dist/extra/context.d.ts"
      ],
      "extra/pathname": [
        "dist/extra/pathname.d.ts"
      ],
      "hoc": [
        "dist/hoc/index.d.ts"
      ],
      "themes": [
        "dist/themes/index.d.ts"
      ],
      "progress-bar": [
        "dist/progress-bar/index.d.ts"
      ],
      "utils": [
        "dist/utils/index.d.ts"
      ],
      "types": [
        "dist/types/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "next",
    "nextjs",
    "next-kit",
    "nextjs-kit",
    "next-utils",
    "react",
    "typescript",
    "javascript",
    "ts",
    "js",
    "utils",
    "utility",
    "kit",
    "kits",
    "@rzl-zone/next-kit"
  ],
  "engines": {
    "node": ">=20.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rzl-zone/next-kit.git"
  },
  "contributors": [
    "Rizalvin Dwiky <rizalvindwiky1998@gmail.com>"
  ],
  "files": [
    "/dist/**/*",
    "!**/*.test.*"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "clean-dist": "del-cli \"dist/*\"",
    "clean-dist:types-cts": "del-cli \"dist/**/*.d.{cts,mts}\"",
    "clean-dist:js-comments": "tsx scripts/cleanDistJsComments.ts",
    "copy:copy-files-dynamics": "tsx scripts/copyFileDynamic.ts",
    "delete:newline": "tsx scripts/removeFinalNewLine.ts",
    "delete-dist:index-runtime": "del-cli \"dist/index.{js,js.map,cjs,cjs.map,esm,esm.map}\" \"dist/index.d.{ts,cts,mts,esm}\"",
    "fix:dts-react-import": "tsx scripts/fixDtsReactImport.ts",
    "inject:banner": "tsx scripts/injectBanner",
    "minify:dts-dist": "tsx scripts/minifyDtsDist.ts",
    "normalize:new-lines": "tsx scripts/normalizeNewLineDistFiles",
    "build:types-dts": "dts-bundle-generator --config dts-config.cjs --silent --external-imports next",
    "generate:reference-index": "tsx scripts/generateReferenceIndex.ts",
    "postbuild": "",
    "build": "npm run clean-dist && tsup && npm run copy:copy-files-dynamics && npm run fix:dts-react-import && npm run clean-dist:js-comments && npm run normalize:new-lines && npm run clean-dist:types-cts && npm run delete-dist:index-runtime && npm run generate:reference-index && npm run inject:banner",
    "format-prettier:write": "prettier --write .",
    "format-prettier:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "test:mocha": "mocha \"**/*.test.ts\" --retries 2",
    "prepublishOnly": "npm run build",
    "check-publish": "npm run build && npm pack --dry-run",
    "release-patch": "npm version patch && git push && git push --tags && npm publish"
  },
  "dependencies": {
    "@edge-runtime/cookies": "^6.0.0",
    "@rzl-zone/ts-types-plus": "latest",
    "@rzl-zone/utils-js": "latest",
    "deepmerge": "^4.3.1",
    "p-safe": "^1.0.0",
    "validate-color": "^2.2.4"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^2.3.5",
    "@eslint/js": "^9.39.1",
    "@types/chai": "^5.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.16.5",
    "@types/react": "^18.3.23",
    "chai": "^5.2.1",
    "chalk": "^5.6.2",
    "del-cli": "^6.0.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.39.1",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "mocha": "^10.8.2",
    "next": "^15.5.4",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "type-fest": "^5.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.47.0"
  },
  "peerDependencies": {
    "next": "14 || 15 || 16",
    "react": "18 || 19"
  }
}
